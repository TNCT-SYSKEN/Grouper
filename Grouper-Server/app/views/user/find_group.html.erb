<div class="container">
<p>招待コードを入力してください</p>
<strong>検索する:</strong><br>
<%= form_tag(url_for(:action => 'find_group'), :id => "invite_form") do %>
  <div class="form-inline">
    <div class="form-group">
      <%= text_field_tag :groupID, "", id: "text", class: "form-control" %>
    </div>
    <%= submit_tag "検索", class: "btn btn-default" %> 
  </div>
<% end %>
<div id="result"></div>
</div>
